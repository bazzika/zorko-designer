{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "moduleResolution": "node",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "typeRoots": ["node_modules/@types"]
  },
  "files": ["src/index.tsx"],
  "include": ["src/**/*Test.ts"]
}
